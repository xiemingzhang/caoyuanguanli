@model GrassPlantInfo.Entity.T_GrassCountyStatistics
@{

    Layout = null;
}
<html>
<head>
    <head title="编辑草情">
        <link rel="stylesheet" type="text/css" href="/js/EasyUi/themes/default/easyui.css" />
        <link rel="stylesheet" type="text/css" href="/js/themes/icon.css" />
        <script src="/js/jquery-1.10.2.min.js"></script>
        <script type="text/javascript" src="/js/EasyUi/jquery.easyui.min.js"></script>
        <script src="/js/EasyUi/easyui-lang-zh_CN.js"></script>
    </head>
</head>
<body>
    <div class="easyui-layout" style="width: 100%; height: 100%;">
        <div style="margin:10px;">
            <h2 style="font-size:14px;">超载率</h2>
            <hr />
            <div id="familylist"></div>
            <div>
                <form id="ff" method="post" action="">
                    <input type="hidden" id="tid" name="Id" />
                    <p style="margin: 0px 0px 20px 0px;">修改超载率</p>
                    <table width="100%" cellspacing="5px">
                        <tr>
                            <td colspan="2">
                                当前编辑县
                                <br/><br/>
                            </td>
                        </tr>
                        <tr>
                            <td width="200px">超载率小于<strong>10%</strong>的草原面积</td>
                            <td>
                                <input type="text" class="easyui-numberbox" name="StateOwnedArea" data-options="required:true" />
                            </td>
                        </tr>
                        <tr>
                            <td width="200px">超载率<strong>10%-15%</strong>的草原面积</td>
                            <td>
                                <input type="text" class="easyui-numberbox" name="StateOwnedArea" data-options="required:true" />
                            </td>
                        </tr>
                        <tr>
                            <td width="200px">超载率大于<strong>15%</strong>的草原面积</td>
                            <td>
                                <input type="text" class="easyui-numberbox" name="StateOwnedArea" data-options="required:true" />
                            </td>
                        </tr>
                        <tr>
                            <td colspan="2" style="padding-left:206px">
                                <a href="javascript:plantFamilyOp.submit()" class="easyui-linkbutton" iconcls="icon-save" plain="true">保存</a>
                            </td>
                        </tr>
                    </table>
                </form>
            </div>
        </div>
</body>
</html>